<UserControl x:Class="Loowoo.Land.OA.TaskClient.Controls.CompleteDate"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Loowoo.Land.OA.TaskClient.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="180">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label Width="130" Padding="23,0,0,0" x:Name="ctrDate" Content="10月20" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" FontSize="24" Foreground="#FF1E559A" >
            <Label.Background>
                <ImageBrush ImageSource="/Loowoo.Land.OA.TaskClient;component/Images/icon_time.png" Stretch="None" AlignmentX="Left"  AlignmentY="Center"/>
            </Label.Background>
        </Label>
        <Border Grid.Column="0" Height="1" VerticalAlignment="Bottom" Style="{DynamicResource BorderStyle1}" />
    </Grid>
</UserControl>
